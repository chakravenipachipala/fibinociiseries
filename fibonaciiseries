#include<stdio.h> 
int main() 
{ 
    int n,i,a,b,c; //defining variables. 
    printf("Enter a number : "); 
    scanf("%d",&n); //taking input. 
    i=1; 
    a=0; 
    b=1; 
    while ( i<=n ) //runs loop till condition becomes false. 
    { 
        printf("%d,",a); 
        c = a + b; 
        a = b; 
        b = c; 
        i++; 
    } 
    return 0; 
} 
